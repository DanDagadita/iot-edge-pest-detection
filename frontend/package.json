{
  "name": "pest-detector",
  "version": "0.0.1",
  "description": "Pest Detector",
  "productName": "Pest Detector",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"src/**/*.{js,ts,vue,scss,html,md,json}\" index.html",
    "formatFile": "prettier --write",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "build-capacitor-android": "cp .env .env.tmp && cp .env.test .env && rm .env.test && quasar build -m capacitor -T android --skip-pkg ; cp .env .env.test && cp .env.tmp .env && rm .env.tmp",
    "prepare": "husky"
  },
  "dependencies": {
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@capgo/capacitor-navigation-bar": "^8.0.5",
    "@quasar/extras": "^1.17.0",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "mqtt": "^5.14.1",
    "pinia": "^3.0.4",
    "quasar": "^2.18.6",
    "validator": "^13.15.26",
    "vue": "^3.4.18",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.4.0",
    "@types/node": "^24.10.4",
    "@types/validator": "^13.15.10",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2",
    "vite-plugin-checker": "^0.12.0",
    "vue-tsc": "^3.2.1"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
